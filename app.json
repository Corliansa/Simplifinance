{
  "expo": {
    "name": "Simplifinance",
    "slug": "Simplifinance",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#041C32"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#041C32"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Simplifinance is compact, fun and easy to use. \nSimply manage your money with Simplifinance!"
  }
}
